target: LEMS_Sim_IClamp_GenericNeuronCellW2D.xml
engine: jNeuroML

mep: .test.ex.mep
experiments:
  state:
    observables:
      spike times:
        file: 
          path: pop_GenericNeuronCellW2D_0.state.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: 0.8
        tolerance: 0.00
  output:
    observables:
      spike times:
        file: 
          path: pop_GenericNeuronCellW2D_0.output.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: 0.8
        tolerance: 0.000

