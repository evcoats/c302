  output:
    observables:
      spike times:
        file: 
          path: pop_GenericNeuronCellW2D_0.output.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: 0.8
        tolerance: 1.0338268133298212e-05

